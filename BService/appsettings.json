{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "BService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "true",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false,
      "health": {
        "enabled": true,
        "checkEnabled": true
      }
    },
    "instance": {
      "port": "7253",
      "ipAddress": "localhost",
      "preferIpAddress": true,
      "healthCheckUrlPath": "/healthcheck"
    }
  }
}
